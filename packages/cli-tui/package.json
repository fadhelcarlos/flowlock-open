{
  "name": "@flowlock/cli-tui",
  "version": "0.10.1",
  "private": false,
  "type": "module",
  "bin": {
    "flowlock": "dist/bin/flowlock.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc -b",
    "lint": "eslint .",
    "test": "vitest run",
    "flowlock:audit": "npx flowlock-uxcg audit",
    "flowlock:audit:fix": "npx flowlock-uxcg audit --fix",
    "flowlock:inventory": "npx flowlock-uxcg inventory",
    "flowlock:diagrams": "npx flowlock-uxcg diagrams"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "chokidar": "^4.0.3",
    "enquirer": "^2.4.1",
    "execa": "^5.1.1",
    "flowlock-shared": "^0.10.0",
    "flowlock-uxcg": "^0.10.0",
    "fuse.js": "^7.0.0",
    "ink": "^5.0.1",
    "ink-select-input": "^6.0.0",
    "ink-text-input": "^6.0.0",
    "marked": "^12.0.2",
    "marked-terminal": "^7.3.0",
    "open": "^8.4.2",
    "react": "^18.3.1",
    "strip-ansi": "^6.0.1",
    "tree-kill": "^1.2.2",
    "zod": "^3.23.8",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@types/ink-select-input": "^3.0.5",
    "@types/ink-text-input": "^2.0.5",
    "@types/node": "^22.17.2",
    "@types/react": "^18.3.23",
    "eslint": "^9.9.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2",
    "vitest": "^2.0.5"
  }
}